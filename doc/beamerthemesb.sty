\mode<presentation>

% Requirement
\RequirePackage{tikz}

% Settings
\useinnertheme{sb}
\useoutertheme{sb}
\usecolortheme{sb}

% disable the top navigation symbols on each page
\setbeamertemplate{navigation symbols}{}
% show rounded boxes with a shadow
\setbeamertemplate{blocks}[rounded][shadow=true]  

\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]

\setbeamertemplate{footline}[sb]


\mode<all>